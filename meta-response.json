{
  "node": {
    "order": [
      "node_id",
      "name",
      "node_type",
      "region",
      "instance_id",
      "size",
      "zone",
      "os",
      "dns_name"
    ],
    "mapping": {
      "name": "display_name",
      "region": "external_ref.region",
      "dns_name": "external_ref.dns_name",
      "size": "external_ref.size.split('.').last",
      "instance_id": "external_ref.instance_id",
      "node_type": "node_type",
      "zone": "external_ref.availability_zone",
      "os": "os_type",
      "node_id": "dtk_id"
    }
  },
  "component": {
    "order": [
      "component_id",
      "name",
      "component_type",
      "version",
      "library",
      "library_id"
    ],
    "mapping": {
      "name": "display_name",
      "version": "version",
      "library": "library.display_name",
      "component_id": "dtk_id",
      "library_id": "library_library_id",
      "component_type": "dtk_type"
    }
  },
  "module": {
    "order": [
      "module_id",
      "name",
      "version"
    ],
    "mapping": {
      "name": "display_name",
      "module_id": "dtk_id",
      "version": "version"
    }
  },
  "node_template": {
    "order": [
      "name",
      "template_name",
      "os",
      "template_type",
      "size"
    ],
    "mapping": {
      "name": "display_name",
      "template_type": "template_type",
      "size": "size.split('.').last",
      "os": "os_type",
      "template_name": "template_name"
    }
  },
  "library": {
    "order": [
      "library_id",
      "library_name"
    ],
    "mapping": {
      "library_name": "display_name",
      "library_id": "dtk_id"
    }
  },
  "remote_module": {
    "order": [
      "name",
      "version"
    ],
    "mapping": {
      "name": "display_name",
      "version": "version"
    }
  },
  "task": {
    "order": [
      "task_id",
      "status",
      "created",
      "start",
      "end"
    ],
    "mapping": {
      "task_id": "dtk_id",
      "end": "ended_at.get_date",
      "start": "started_at.get_date",
      "status": "status.upcase",
      "created": "created_at.get_date"
    }
  },
  "target": {
    "order": [
      "target_id",
      "target_type",
      "iaas",
      "description"
    ],
    "mapping": {
      "iaas": "iaas_type",
      "target_id": "dtk_id",
      "description": "description",
      "target_type": "dtk_type"
    }
  },
  "assembly": {
    "order": [
      "assembly_id",
      "assembly_name",
      "nodes",
      "components"
    ],
    "mapping": {
      "components": "nodes.first['components'].join(', ')",
      "nodes": "nodes.size",
      "assembly_name": "display_name",
      "assembly_id": "dtk_id"
    }
  }
}