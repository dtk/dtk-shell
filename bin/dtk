#!/usr/bin/env ruby
require File.expand_path('../lib/shell/help_monkey_patch',                   File.dirname(__FILE__))

# take command to be executed, rest of arguments are going to be used 
# as provided arguments. Under DTK task there is summarization of all
# task that are available.
command = ARGV.shift if ARGV.size > 0

require File.expand_path('../lib/client', File.dirname(__FILE__))
top_level_execute(command)